{
  "expo": {
    "name": "CosmoLens",
    "slug": "ldapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cosmolens.png",
    "scheme": "cosmolens",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/cosmolens.png",
        "backgroundImage": "./assets/images/cosmolens.png",
        "monochromeImage": "./assets/images/cosmolens.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "earth.ldaorbita.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favcosmolens.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-screen.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e467a261-7a76-4063-b131-cbae264b232e"
      }
    },
    "owner": "gafc2001"
  }
}
